## Template for deploy environment variables
# Copy this file to `deploy.env` and replace placeholders with real secrets before running `deploy.ps1`.

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Paysera (if used)
PAYSERA_PROJECT_ID=your_paysera_project_id
PAYSERA_SIGN_PASSWORD=your_paysera_sign_password

# App configuration
HOST_URL=https://your-site.example
PORT=5000

# JWT secret
JWT_SECRET=replace_with_a_long_random_string

# Docker / Compose specific (optional)
NODE_ENV=production
